<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="Coldew.Data.Organization" assembly="Coldew.Data">
    <class name="SignInLogModel" table="org_authlog">
        <id name="ID" type="long" column="sign_id" unsaved-value="0">
            <generator class="native">
            </generator>
        </id>
        <property name="Account" type="String" column="sign_account"></property>
        <property name="SignInResult" type="int" column="sign_result"></property>
        <property name="SignInTime" type="DateTime" column="sign_time"></property>
        <property name="Ip" type="string" column="sign_clientIp"></property>
    </class>
</hibernate-mapping>