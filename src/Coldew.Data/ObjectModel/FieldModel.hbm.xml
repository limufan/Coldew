<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="Coldew.Data" assembly="Coldew.Data">
    <class name="FieldModel" table="coldew_field">
        <id name="ID" type="string" column="field_id"></id>
        <property name="ObjectId" type="string" column="object_id"></property>
        <property name="Name" type="String" column="field_name"></property>
        <property name="Tip" type="String" column="field_tip"></property>
        <property name="Code" type="String" column="field_code"></property>
        <property name="Required" type="bool" column="field_required"></property>
        <property name="IsSystem" type="bool" column="field_isSystem"></property>
        <property name="IsSummary" type="bool" column="field_isSummary"></property>
        <property name="GridWidth" type="int" column="field_gridWidth"></property>
        <property name="Type" type="string" column="field_type"></property>
        <property name="Unique" type="bool" column="field_unique"></property>
        <property name="Config" type="String" length="20000" column="field_config"></property>
    </class>
</hibernate-mapping>