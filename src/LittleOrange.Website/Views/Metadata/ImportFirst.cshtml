@using Coldew.Website;
@using Coldew.Website.Models;
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    this.ViewBag.TopMenu = "Metadata";
}
<div class="container-fluid">
    <p class="lead">@this.ViewBag.Title</p>
    <form enctype="multipart/form-data" action="@this.Url.Action("UploadImportFile")" method="post">
         <fieldset>
            <legend>第一步</legend>
            <a class="btn btn-default" href="@this.Url.Action("DownloadImportTemplate", new {objectId = this.Request["objectId"]})"><i class="icon-download"></i>下载模板</a>
        </fieldset>
         <fieldset>
            <legend>第二步</legend>
            <input type="hidden" name="objectId" value="@this.Request["objectId"]"/>
            <input type="file" name="importFile"/>
            <button class="btn btn-default" ><i class="icon-upload"></i>上传</button>
        </fieldset>
    </form>
<//div>